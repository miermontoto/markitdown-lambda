# AWS Configuration
AWS_RUNTIME=python3.11
AWS_ARCH=arm64
AWS_MAIN_REGION=eu-west-1
AWS_MEMORY_SIZE=1024
AWS_TIMEOUT_IN_SECS=300

# S3 Configuration
S3_BUCKET_NAME=markitdown-bucket

# Custom Domain (optional)
CUSTOM_SUBDOMAIN=subdomain
CUSTOM_DOMAIN=yourdomain.com
AWS_CUSTOM_DOMAIN_ZONE_ID=XXXXXXXXXXXXXXXXXX

# Application
APP_NAME=markitdown
APP_VERSION=1.0.0

# Security (optional - if not set, API will be open)
API_KEY=your-secret-api-key-here
